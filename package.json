{
    "name": "find-this-lines-owner",
    "displayName": "find this line's owner",
    "description": "finds who is the original owner of each line of code",
    "version": "0.0.1",
    "publisher": "HLabs",
    "repository": {
        "type": "git",
        "url": "https://github.com/hhazani/find-this-lines-owner"
    },
    "engines": {
        "vscode": "^1.106.1"
    },
    "categories": [
        "Other"
    ],
    "keywords": [
        "git",
        "blame",
        "history",
        "author",
        "ownership",
        "codelens"
    ],
    "icon": "icon.png",
    "license": "MIT",
    "activationEvents": [],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "find-this-lines-owner.investigate-line",
                "title": "Find This Line's Owner"
            },
            {
                "command": "find-this-lines-owner.show-history",
                "title": "Show Line History"
            }
        ],
        "menus": {
            "editor/context": [
                {
                    "command": "find-this-lines-owner.investigate-line",
                    "when": "editorTextFocus",
                    "group": "navigation@1"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src",
        "test": "vscode-test"
    },
    "devDependencies": {
        "@types/vscode": "^1.106.1",
        "@types/mocha": "^10.0.10",
        "@types/node": "22.x",
        "typescript-eslint": "^8.46.3",
        "eslint": "^9.39.1",
        "typescript": "^5.9.3",
        "@vscode/test-cli": "^0.0.12",
        "@vscode/test-electron": "^2.5.2"
    }
}
